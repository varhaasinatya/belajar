-- --------------------------------------------------------

--
-- Structure for view `network_position_stats`
--
DROP TABLE IF EXISTS `network_position_stats`;

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `network_position_stats`  AS SELECT `users`.`parent_id` AS `parent_id`, `users`.`position` AS `position`, count(0) AS `position_count` FROM `users` WHERE `users`.`parent_id` is not null GROUP BY `users`.`parent_id`, `users`.`position` ORDER BY `users`.`parent_id` ASC, `users`.`position` ASC ;

-- --------------------------------------------------------

--
-- Structure for view `network_structure_view`
--
DROP TABLE IF EXISTS `network_structure_view`;

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `network_structure_view`  AS SELECT `u`.`id` AS `id`, `u`.`username` AS `username`, `u`.`parent_id` AS `parent_id`, `u`.`referral_id` AS `referral_id`, `p`.`username` AS `parent_username`, `r`.`username` AS `referrer_username`, `u`.`leg` AS `leg`, `u`.`position` AS `position`, `u`.`status` AS `status`, `u`.`product_id` AS `product_id`, `pr`.`nama_produk` AS `product_name`, `u`.`join_date` AS `join_date`, `u`.`poin` AS `poin`, (select count(0) from `users` where `users`.`parent_id` = `u`.`id`) AS `direct_children_count` FROM (((`users` `u` left join `users` `p` on(`u`.`parent_id` = `p`.`id`)) left join `users` `r` on(`u`.`referral_id` = `r`.`id`)) left join `products` `pr` on(`u`.`product_id` = `pr`.`id`)) ORDER BY `u`.`id` ASC ;

