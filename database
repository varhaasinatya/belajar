--
-- Table structure for table `user_children`
--

CREATE TABLE `user_children` (
  `parent_id` int(11) NOT NULL,
  `child_id` int(11) NOT NULL,
  `position` varchar(20) NOT NULL,
  `created_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

-- --------------------------------------------------------

--
-- Table structure for table `user_rewards`
--

CREATE TABLE `user_rewards` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `reward_id` int(11) NOT NULL,
  `reward_type` enum('produk','aksi','global') NOT NULL DEFAULT 'produk',
  `point` int(11) NOT NULL DEFAULT 0,
  `reward_amount` decimal(12,2) NOT NULL DEFAULT 0.00,
  `status` enum('pending','approved','withdrawn','expired') DEFAULT 'pending',
  `note` varchar(255) DEFAULT NULL,
  `created_at` datetime DEFAULT current_timestamp(),
  `approved_at` datetime DEFAULT NULL,
  `withdrawn_at` datetime DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

-- --------------------------------------------------------

--
-- Table structure for table `whitelist_global`
--

CREATE TABLE `whitelist_global` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `entry_date` datetime NOT NULL DEFAULT current_timestamp(),
  `position` int(11) NOT NULL COMMENT 'Posisi dalam whitelist (1-9)',
  `status` enum('active','promoted','expired') DEFAULT 'active'
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

-- --------------------------------------------------------

--
-- Table structure for table `withdrawal`
--

CREATE TABLE `withdrawal` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `amount` decimal(18,2) NOT NULL,
  `type` enum('komisi','reward','bonus') DEFAULT 'komisi',
  `reward_id` int(11) DEFAULT NULL,
  `status` enum('pending','approved','rejected') DEFAULT 'pending',
  `note` text DEFAULT NULL,
  `bukti_transfer` varchar(255) DEFAULT NULL,
  `admin_id` int(11) DEFAULT NULL,
  `approved_at` datetime DEFAULT NULL,
  `invoice_no` varchar(40) DEFAULT NULL,
  `created_at` datetime DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

