  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `username` (`username`),
  ADD UNIQUE KEY `user_referral_code` (`user_referral_code`),
  ADD UNIQUE KEY `idx_parent_leg` (`parent_id`,`leg`),
  ADD KEY `idx_referral_id` (`referral_id`),
  ADD KEY `idx_parent_id` (`parent_id`),
  ADD KEY `idx_status` (`status`),
  ADD KEY `idx_product_id` (`product_id`),
  ADD KEY `fk_users_child_left` (`child_left`),
  ADD KEY `fk_users_child_center` (`child_center`),
  ADD KEY `fk_users_child_right` (`child_right`),
  ADD KEY `idx_users_position` (`position`),
  ADD KEY `idx_users_leg` (`leg`),
  ADD KEY `idx_parent_position` (`parent_id`,`position`),
